{
  "name": "videojs-wavesurfer",
  "version": "2.0.0",
  "description": "video.js plugin that adds a navigable waveform for audio and video files.",
  "author": "Collab",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/collab-project/videojs-wavesurfer/issues"
  },
  "homepage": "https://github.com/collab-project/videojs-wavesurfer",
  "main": "es5/videojs.wavesurfer.js",
  "directories": {
    "examples": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collab-project/videojs-wavesurfer.git"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:js": "npm-run-all build:js:babel build:js:browserify build:js:bannerize build:js:collapse build:js:uglify",
    "build:js:babel": "babel src -d es5",
    "build:js:bannerize": "bannerize dist/videojs.wavesurfer.js --banner=scripts/banner.ejs",
    "build:js:browserify": "browserify . --standalone videojs.wavesurfer -g browserify-shim -o dist/videojs.wavesurfer.js",
    "build:js:collapse": "bundle-collapser dist/videojs.wavesurfer.js -o dist/videojs.wavesurfer.min.js",
    "build:js:uglify": "uglifyjs dist/videojs.wavesurfer.min.js --support-ie8 --comments -m -c -o dist/videojs.wavesurfer.min.js",
    "build:test:browserify": "browserify test/browserify-test.js -o dist-test/browserify-test.js",
    "build:test:webpack": "webpack test/webpack-test.js dist-test/webpack-test.js",
    "build:test": "npm-run-all build:test:js build:test:browserify build:test:webpack",
    "build:test:js": "node scripts/build-test.js",
    "clean": "npm-run-all -p clean:*",
    "clean:build": "node -e \"var s=require('shelljs'),d=['dist','dist-test','es5'];s.rm('-rf',d);s.mkdir('-p',d);\"",
    "docs": "npm-run-all docs:*",
    "docs:api": "jsdoc src -r -d docs/api",
    "lint": "jscs --config .jscsrc src",
    "start": "npm-run-all -p watch start:*",
    "start:serve": "babel-node scripts/server.js",
    "pretest": "npm-run-all lint build",
    "preversion": "npm test",
    "version": "npm run build",
    "watch": "npm-run-all -p watch:*",
    "watch:docs": "nodemon --watch src/ --exec npm run docs",
    "watch:js": "npm-run-all -p watch:js:babel watch:js:browserify",
    "watch:js:babel": "npm run build:js:babel -- --watch",
    "watch:js:browserify": "watchify . -v -g browserify-shim -o dist/videojs.wavesurfer.js",
    "watch:test:browserify": "watchify test/browserify-test.js -o dist-test/browserify-test.js",
    "watch:test:webpack": "watchify test/webpack-test.js -o dist-test/webpack-test.js",
    "watch:test": "npm-run-all -p watch:test:js watch:test:webpack watch:test:browserify",
    "watch:test:js": "node scripts/watch-test.js",
    "prepublish": "not-in-install && npm run build || in-install"
  },
  "files": [
    "dist/",
    "docs/",
    "es5/",
    "scripts/",
    "src/",
    "test/"
  ],
  "keywords": [
    "waveform",
    "audio",
    "video",
    "wavesurfer",
    "videojs",
    "videojs-plugin",
    "player"
  ],
  "browserify-shim": {
    "qunit": "global:QUnit",
    "sinon": "global:sinon",
    "video.js": "global:videojs",
    "wavesurfer.js": "global:WaveSurfer"
  },
  "dependencies": {
    "video.js": ">=6.0.0",
    "wavesurfer.js": "^2.0.0-beta01"
  },
  "devDependencies": {
    "babel": "^5.8.0",
    "babelify": "^6.0.0",
    "bannerize": "^1.0.0",
    "browserify": "^11.0.0",
    "browserify-istanbul": "^2.0.0",
    "browserify-shim": "^3.0.0",
    "bundle-collapser": "^1.2.1",
    "connect": "^3.4.0",
    "d3": "^3.4.8",
    "glob": "^6.0.3",
    "in-publish": "^2.0.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jsdoc": "^3.4.0",
    "lodash": "^4.17.4",
    "lodash-compat": "^3.10.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.2.0",
    "portscanner": "^1.0.0",
    "qunitjs": "^2.0.1",
    "serve-static": "^1.10.0",
    "shelljs": "^0.5.3",
    "sinon": "1.10.2",
    "uglify-js": "^2.5.0",
    "watchify": "^3.6.0",
    "webpack": "^1.13.2"
  }
}
